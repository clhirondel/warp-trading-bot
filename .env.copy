# Wallet
PRIVATE_KEY=

# Connection
RPC_ENDPOINT=https://api.mainnet-beta.solana.com
RPC_WEBSOCKET_ENDPOINT=wss://api.mainnet-beta.solana.com
COMMITMENT_LEVEL=confirmed

# Bot
LOG_LEVEL=trace
ONE_TOKEN_AT_A_TIME=true
PRE_LOAD_EXISTING_MARKETS=false
CACHE_NEW_MARKETS=false
# default or warp or jito
TRANSACTION_EXECUTOR=default
# if using default executor, fee below will be applied
COMPUTE_UNIT_LIMIT=101337
COMPUTE_UNIT_PRICE=421197
# if using warp or jito executor, fee below will be applied
CUSTOM_FEE=0.006

# --- Telegram Alerts (Optional) --- #
# Set to true to enable Telegram alerts
TELEGRAM_ALERTS_ENABLED=false
# Your Telegram Bot Token (get from BotFather)
TELEGRAM_BOT_TOKEN=
# Your Telegram Chat ID (get from userinfobot or similar)
TELEGRAM_CHAT_ID=

# Buy
QUOTE_MINT=So11111111111111111111111111111111111111112
QUOTE_AMOUNT=0.01
AUTO_BUY_DELAY=0
MAX_BUY_RETRIES=10
BUY_SLIPPAGE=10
# Set to false to only monitor and log filter results without executing buys
AUTO_BUY=true

# Sell
AUTO_SELL=true
MAX_SELL_RETRIES=10
AUTO_SELL_DELAY=0
PRICE_CHECK_INTERVAL=2000
PRICE_CHECK_DURATION=600000
TAKE_PROFIT=25
STOP_LOSS=10
TAKE_PROFIT_PERCENTAGE=0 # Percentage gain (e.g., 100 for 100%). Set to 0 to disable.
STOP_LOSS_PERCENTAGE=15  # Percentage loss (e.g., 50 for 50%). Set to 0 to disable.
SELL_SLIPPAGE=10

# --- Time-based Auto Sell --- #
MAX_SELL_DURATION_SECONDS=120 # Default: Sell automatically after 120 seconds (2 minutes), regardless of PnL. 0 to disable.
SELL_TIMED_NAME_KEYWORDS=Trump # Comma-separated keywords in token name for faster timed sell.
SELL_TIMED_NAME_DURATION_SECONDS=60 # Sell after 60 seconds (1 minute) if name contains keywords.

# Filters
USE_SNIPE_LIST=false
SNIPE_LIST_REFRESH_INTERVAL=30000
FILTER_CHECK_DURATION=60000
FILTER_CHECK_INTERVAL=2000
CONSECUTIVE_FILTER_MATCHES=3
MAX_POOL_AGE_SECONDS=1200 # Maximum age of the pool in seconds (e.g., 1200 for 20 minutes). Set to 0 to disable.
CHECK_IF_MUTABLE=false
CHECK_IF_SOCIALS=true
CHECK_IF_MINT_IS_RENOUNCED=true
CHECK_IF_FREEZABLE=true
CHECK_IF_BURNED=true
MIN_POOL_SIZE=5
MAX_POOL_SIZE=50
# Minimum market cap in quote token (e.g., 10000 for $10k). Set to 0 to disable.
MIN_MARKET_CAP=0
# --- Priority Fees --- #
USE_PRIORITY_FEES=true
# Example: Using micro-lamports directly (adjust values as needed)
BUY_PRIORITY_FEE_MICROLAMPORTS=24700000 # To get ~2.5M Lamport total fee with a limit of ~101k
SELL_PRIORITY_FEE_MICROLAMPORTS=14800000 # To get ~1.5M Lamport total fee with a limit of ~101k

# ATA specific fees (micro-lamports per unit)
ATA_PRIORITY_FEE_MAX_MICROLAMPORTS=1500000

COMPUTE_UNIT_LIMIT=101337
COMPUTE_UNIT_PRICE=421197

# --- Filtering: Blocklists (Comma-separated, Case-insensitive) --- #
FILTER_BLOCKLIST_NAMES=PUMP
FILTER_BLOCKLIST_SYMBOLS=TRUMP
